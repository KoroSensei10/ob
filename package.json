{
  "name": "ob",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test": "pnpm run test:unit -- --run && pnpm run test:e2e",
    "test:e2e": "pnpm prepare:test && playwright test",
		"prepare:db": "tsx --env-file=.env scripts/setup-db.ts",
    "prepare:test": "tsx --env-file=.env.test scripts/setup-test-db.ts",
    "lint": "pnpm check && eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prepare": "svelte-kit sync || echo ''"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
